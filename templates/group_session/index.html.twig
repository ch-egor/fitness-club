{% extends 'base.html.twig' %}

{% block title %}GroupSession index{% endblock %}

{% block body %}
    <h1>GroupSession index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Coach</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for group_session in group_sessions %}
            <tr>
                <td>{{ group_session.id }}</td>
                <td>{{ group_session.name }}</td>
                <td>{{ group_session.coach }}</td>
                <td>{{ group_session.description }}</td>
                <td>
                    <a href="{{ path('group_session_show', {'id': group_session.id}) }}">show</a>
                    <a href="{{ path('group_session_edit', {'id': group_session.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('group_session_new') }}">Create new</a>
{% endblock %}
