<form method="post" action="{{ path('group_session_delete', {'id': group_session.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ group_session.id) }}">
    <button class="btn">Delete</button>
</form>
